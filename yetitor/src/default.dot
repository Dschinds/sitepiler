<html>
	<head>
		<link rel="stylesheet" href="https://d3a63qt71m2kua.cloudfront.net/developercenter/1879/styles/devcenter.css" />
		<style>
			body {
				padding: 15px !important;
			}

			.devcenter-content {
				margin: 0;
				padding: 10px 0;
			}
		</style>
		<!-- jQuery -->
		<script
			src="https://code.jquery.com/jquery-3.3.1.min.js"
			integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
			crossorigin="anonymous"
		></script>
		<script
			src="{{= context.origin }}/scripts/prism.js"
		></script>
		<script type="text/javascript">
			// Code fence collapser
			$(document).ready(() => {
				$('.fence > .fence-header').click(function(event) {
					event.preventDefault();
					$(this)
						.parent()
						.children('.fence-body')
						.toggle({ easing: 'linear', duration: 300 });
				});
			});
		</script>
	</head>
	<body>{{? context.page.hideTitle !== true }}
		<h1 class="page-title">{{= context.page.title }}</h1>{{?}}
		<div class="devcenter-body">
			<div class="devcenter-content">
				<div id="devcenter-search-content">
					{{= context.content }}
				</div>
			</div>
		</div>
	</body>
</html>
